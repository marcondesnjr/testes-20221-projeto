<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <rule>
       <from>/editar/usr/</from>
       <to>/control?command=EditaUsuario</to>
    </rule>
    <rule>
       <from>/usr/(.*)</from>
       <to>/control?command=ExbUsuario&amp;email=$1</to>
    </rule>
    <rule>
       <from>/index/</from>
       <to>/control?command=Index</to>
    </rule>
    <rule>
       <from>/filme/([0-9]+)</from>
       <to>/control?command=ExbFilme&amp;id=$1</to>
    </rule>
    <rule>
       <from>/home/</from>
       <to>/control?command=Home</to>
    </rule>
    <rule>
       <from>/singin/page/adm/</from>
       <to>/control?command=SingInPageAdm</to>
    </rule>
    <rule>
       <from>/singin/page</from>
       <to>/control?command=SingInPage</to>
    </rule>
    <rule>
       <from>/singin/send/</from>
       <to>/control?command=SingIn</to>
    </rule>
    <rule>
       <from>/sol/acc/(.*)</from>
       <to>/control?command=AceitarSolicitacao&amp;email=$1</to>
    </rule>
    <rule>
       <from>/sol/rem/(.*)</from>
       <to>/control?command=RejeitarSolicitacao&amp;email=$1</to>
    </rule>
    <rule>
       <from>/sol/(.*)</from>
       <to>/control?command=SolicitarAmizade&amp;email=$1</to>
    </rule>
    <rule>
       <from>/cadastrar/filme/send/</from>
       <to>/control?command=AddFilme</to>
    </rule>
    <rule>
       <from>/cadastrar/filme/</from>
       <to>/control?command=ExbCadastrarFilme</to>
    </rule>
    <rule>
       <from>busca/filme/send/</from>
       <to>/control?command=BuscaFilme</to>
    </rule>
    <rule>
       <from>/busca/filme/</from>
       <to>/control?command=ExbBuscaFilme</to>
    </rule>
    <rule>
       <from>/cadastrar/grupo/</from>
       <to>/control?command=AddGrupo</to>
    </rule>
    
    <rule>
       <from>filme/avaliar/</from>
       <to>/control?command=AvaliarFilme</to>
    </rule>
    <rule>
       <from>/recomendar/</from>
       <to>/control?command=AddRecomendacao</to>
    </rule>
     <rule>
       <from>/login/</from>
       <to>/control?command=LogIn</to>
    </rule>
    <rule>
       <from>/logoff/</from>
       <to>/control?command=LogOff</to>
    </rule>
    <rule>
       <from>/busca/usuario/</from>
       <to>/control?command=BuscaUsuario</to>
    </rule>
    <rule>
       <from>/participar/grupo/([0-9]+)</from>
       <to>/control?command=ParticiparGrupo&amp;id=$1</to>
    </rule>
    <rule>
       <from>/grupo/([0-9]+)</from>
       <to>/control?command=ExbGrupo&amp;id=$1</to>
    </rule>
    <rule>
       <from>/busca/grupo/page/</from>
       <to>/control?command=ExbBuscaGrupo</to>
    </rule>
    
    <rule>
       <from>/busca/grupo/send/</from>
       <to>/control?command=BuscaGrupo</to>
    </rule>
     <rule>
       <from>/del/</from>
       <to>/control?command=RemUsuario</to>
    </rule>
</urlrewrite>
